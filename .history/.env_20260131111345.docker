# =============================================================================
# DOCKER ENVIRONMENT VARIABLES
# Copy this to .env before running docker-compose up
# =============================================================================

# Database passwords
MYSQL_ROOT_PASSWORD=your_root_password_change_this
DB_PASSWORD=your_db_password_change_this

# Laravel App Key (generate with: docker-compose exec backend php artisan key:generate)
APP_KEY=base64:GENERATE_THIS_AFTER_FIRST_RUN
